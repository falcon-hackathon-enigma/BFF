{
  "root": true,
  "rules": {
    "quotes": ["error", "single"],
    "object-curly-spacing": ["error", "always"],
    "comma-dangle": ["error", "never"],
    "import/no-named-as-default": 0
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:node/recommended",
    "plugin:promise/recommended"
  ],
  "ignorePatterns": ["dist/*"]
}
